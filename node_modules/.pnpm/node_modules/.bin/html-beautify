#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yby/Desktop/test/node/koa/demo002/koa2-starter/node_modules/.pnpm/js-beautify@1.14.7/node_modules/js-beautify/js/bin/node_modules:/Users/yby/Desktop/test/node/koa/demo002/koa2-starter/node_modules/.pnpm/js-beautify@1.14.7/node_modules/js-beautify/js/node_modules:/Users/yby/Desktop/test/node/koa/demo002/koa2-starter/node_modules/.pnpm/js-beautify@1.14.7/node_modules/js-beautify/node_modules:/Users/yby/Desktop/test/node/koa/demo002/koa2-starter/node_modules/.pnpm/js-beautify@1.14.7/node_modules:/Users/yby/Desktop/test/node/koa/demo002/koa2-starter/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/yby/Desktop/test/node/koa/demo002/koa2-starter/node_modules/.pnpm/js-beautify@1.14.7/node_modules/js-beautify/js/bin/node_modules:/Users/yby/Desktop/test/node/koa/demo002/koa2-starter/node_modules/.pnpm/js-beautify@1.14.7/node_modules/js-beautify/js/node_modules:/Users/yby/Desktop/test/node/koa/demo002/koa2-starter/node_modules/.pnpm/js-beautify@1.14.7/node_modules/js-beautify/node_modules:/Users/yby/Desktop/test/node/koa/demo002/koa2-starter/node_modules/.pnpm/js-beautify@1.14.7/node_modules:/Users/yby/Desktop/test/node/koa/demo002/koa2-starter/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../js-beautify/js/bin/html-beautify.js" "$@"
else
  exec node  "$basedir/../js-beautify/js/bin/html-beautify.js" "$@"
fi
