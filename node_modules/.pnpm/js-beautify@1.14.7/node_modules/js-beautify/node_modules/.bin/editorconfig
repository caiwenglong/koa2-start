#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yby/Desktop/test/node/koa/demo002/koa2-starter/node_modules/.pnpm/editorconfig@0.15.3/node_modules/editorconfig/bin/node_modules:/Users/yby/Desktop/test/node/koa/demo002/koa2-starter/node_modules/.pnpm/editorconfig@0.15.3/node_modules/editorconfig/node_modules:/Users/yby/Desktop/test/node/koa/demo002/koa2-starter/node_modules/.pnpm/editorconfig@0.15.3/node_modules:/Users/yby/Desktop/test/node/koa/demo002/koa2-starter/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/yby/Desktop/test/node/koa/demo002/koa2-starter/node_modules/.pnpm/editorconfig@0.15.3/node_modules/editorconfig/bin/node_modules:/Users/yby/Desktop/test/node/koa/demo002/koa2-starter/node_modules/.pnpm/editorconfig@0.15.3/node_modules/editorconfig/node_modules:/Users/yby/Desktop/test/node/koa/demo002/koa2-starter/node_modules/.pnpm/editorconfig@0.15.3/node_modules:/Users/yby/Desktop/test/node/koa/demo002/koa2-starter/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../editorconfig@0.15.3/node_modules/editorconfig/bin/editorconfig" "$@"
else
  exec node  "$basedir/../../../../../editorconfig@0.15.3/node_modules/editorconfig/bin/editorconfig" "$@"
fi
